{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/getProducts",
      "dest": "/api/getProducts.js"
    },
    {
      "src": "/api/addProduct",
      "dest": "/api/addProduct.js"
    },
    {
      "src": "/api/updateProduct",
      "dest": "/api/updateProduct.js"
    },
    {
      "src": "/api/delProduct",
      "dest": "/api/delProduct.js"
    },
    {
      "src": "/api/getRoleByUser",
      "dest": "/api/getRoleByUser.js"
    },
    {
      "src": "/api/seedProducts",
      "dest": "/api/seedProducts.js"
    },
    {
      "src": "/api/neonGet",
      "dest": "/api/neonGet.js"
    },
    {
      "src": "/api/neonSet",
      "dest": "/api/neonSet.js"
    },
    {
      "src": "/api/neonDel",
      "dest": "/api/neonDel.js"
    }
  ]
}
